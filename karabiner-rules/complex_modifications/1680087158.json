{
  "title": "MacBook Arrow Keys",
  "rules": [
    {
      "description": "MacBook native keyboard arrow keys (fn + w/a/s/d to arrow keys)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": []
            }
          },
          "to": [{ "key_code": "up_arrow" }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": []
            }
          },
          "to": [{ "key_code": "left_arrow" }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": []
            }
          },
          "to": [{ "key_code": "down_arrow" }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": ["fn"],
              "optional": []
            }
          },
          "to": [{ "key_code": "right_arrow" }]
        }
      ]
    }
  ]
}
